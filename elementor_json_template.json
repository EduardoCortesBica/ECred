{
  "version": "0.4",
  "title": "VIP Financeira Template",
  "type": "page",
  "content": [
    {
      "id": "header-section",
      "elType": "section",
      "settings": {
        "background_background": "classic",
        "background_color": "#000000",
        "position": "fixed",
        "z_index": 1000,
        "box_shadow_box_shadow_type": "yes",
        "box_shadow_box_shadow": {
          "horizontal": 0,
          "vertical": 2,
          "blur": 10,
          "spread": 0,
          "color": "rgba(0,0,0,0.3)"
        }
      },
      "elements": [
        {
          "id": "header-column",
          "elType": "column",
          "settings": {
            "width": "100"
          },
          "elements": [
            {
              "id": "logo-widget",
              "elType": "widget",
              "widgetType": "image",
              "settings": {
                "image": {
                  "url": "logo.webp"
                },
                "width": {
                  "size": 120,
                  "unit": "px"
                },
                "align": "left"
              }
            },
            {
              "id": "nav-menu",
              "elType": "widget",
              "widgetType": "nav-menu",
              "settings": {
                "layout": "horizontal",
                "align_items": "center",
                "background_background": "gradient",
                "background_color": "#4CAF50",
                "background_color_b": "#45a049",
                "border_radius": {
                  "size": 25,
                  "unit": "px"
                },
                "padding": {
                  "top": 10,
                  "right": 20,
                  "bottom": 10,
                  "left": 20,
                  "unit": "px"
                }
              }
            }
          ]
        }
      ]
    },
    {
      "id": "hero-section",
      "elType": "section",
      "settings": {
        "background_background": "gradient",
        "background_color": "#1a1a1a",
        "background_color_b": "#2d2d2d",
        "background_gradient_angle": {
          "size": 135,
          "unit": "deg"
        },
        "padding": {
          "top": 150,
          "right": 0,
          "bottom": 100,
          "left": 0,
          "unit": "px"
        },
        "content_position": "middle",
        "align": "center"
      },
      "elements": [
        {
          "id": "hero-column",
          "elType": "column",
          "settings": {
            "width": "100"
          },
          "elements": [
            {
              "id": "hero-badge",
              "elType": "widget",
              "widgetType": "heading",
              "settings": {
                "title": "Conte com a Vip",
                "size": "small",
                "background_background": "gradient",
                "background_color": "#f39c12",
                "background_color_b": "#e67e22",
                "color": "#000000",
                "border_radius": {
                  "size": 25,
                  "unit": "px"
                },
                "padding": {
                  "top": 8,
                  "right": 25,
                  "bottom": 8,
                  "left": 25,
                  "unit": "px"
                }
              }
            },
            {
              "id": "hero-title",
              "elType": "widget",
              "widgetType": "heading",
              "settings": {
                "title": "Qual o tamanho do seu sonho?",
                "size": "xxl",
                "typography_font_size": {
                  "size": 56,
                  "unit": "px"
                },
                "typography_font_weight": "700",
                "text_shadow_text_shadow_type": "yes",
                "text_shadow_text_shadow": {
                  "horizontal": 2,
                  "vertical": 2,
                  "blur": 4,
                  "color": "rgba(0,0,0,0.5)"
                },
                "_animation": "fadeInUp"
              }
            },
            {
              "id": "hero-subtitle",
              "elType": "widget",
              "widgetType": "text-editor",
              "settings": {
                "editor": "Trabalhamos para ajudar pessoas como você a ter uma vida melhor a partir do crédito consignado.",
                "typography_font_size": {
                  "size": 21,
                  "unit": "px"
                },
                "color": "#cccccc",
                "width": {
                  "size": 600,
                  "unit": "px"
                },
                "margin": {
                  "top": 0,
                  "right": "auto",
                  "bottom": 40,
                  "left": "auto",
                  "unit": "px"
                }
              }
            },
            {
              "id": "hero-buttons",
              "elType": "widget",
              "widgetType": "button",
              "settings": {
                "text": "Fale Conosco",
                "background_background": "gradient",
                "background_color": "#4CAF50",
                "background_color_b": "#45a049",
                "border_radius": {
                  "size": 30,
                  "unit": "px"
                },
                "padding": {
                  "top": 15,
                  "right": 30,
                  "bottom": 15,
                  "left": 30,
                  "unit": "px"
                },
                "box_shadow_box_shadow_type": "yes",
                "box_shadow_box_shadow": {
                  "horizontal": 0,
                  "vertical": 4,
                  "blur": 15,
                  "spread": 0,
                  "color": "rgba(76, 175, 80, 0.3)"
                }
              }
            }
          ]
        }
      ]
    },
    {
      "id": "stats-section",
      "elType": "section",
      "settings": {
        "background_background": "gradient",
        "background_color": "#2d2d2d",
        "background_color_b": "#1a1a1a",
        "background_gradient_angle": {
          "size": 135,
          "unit": "deg"
        },
        "padding": {
          "top": 80,
          "right": 0,
          "bottom": 80,
          "left": 0,
          "unit": "px"
        },
        "content_position": "middle",
        "align": "center"
      },
      "elements": [
        {
          "id": "stats-column-1",
          "elType": "column",
          "settings": {
            "width": "25"
          },
          "elements": [
            {
              "id": "counter-1",
              "elType": "widget",
              "widgetType": "counter",
              "settings": {
                "starting_number": 0,
                "ending_number": 60000,
                "prefix": "+",
                "suffix": "mil",
                "title": "Clientes atendidos no mês",
                "number_color": "#f39c12",
                "title_color": "#cccccc",
                "background_background": "classic",
                "background_color": "rgba(255, 255, 255, 0.05)",
                "border_radius": {
                  "size": 15,
                  "unit": "px"
                },
                "padding": {
                  "top": 30,
                  "right": 30,
                  "bottom": 30,
                  "left": 30,
                  "unit": "px"
                }
              }
            }
          ]
        },
        {
          "id": "stats-column-2",
          "elType": "column",
          "settings": {
            "width": "25"
          },
          "elements": [
            {
              "id": "counter-2",
              "elType": "widget",
              "widgetType": "counter",
              "settings": {
                "starting_number": 0,
                "ending_number": 200,
                "prefix": "+",
                "title": "colaboradores",
